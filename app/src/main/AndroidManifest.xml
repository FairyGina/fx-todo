<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.fx_todo">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher_dambi"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_dambi_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Fxtodo"
        tools:targetApi="31">
        <activity
            android:name=".Canvas"
            android:exported="false" />
        <activity
            android:name=".CanvasView"
            android:exported="false" />
        <activity
            android:name=".LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".WeekViewActivity" />
        <activity android:name=".OptionActivity" />
        <activity android:name=".PlusActivity" />
        <activity android:name=".PriorityGraphActivity" />
        <activity android:name=".AccountActivity" />
        <activity android:name=".EventEditActivity" />
        <activity
            android:name=".LogoutActivity"
            android:theme="@style/Theme.MyDialog" />
        <activity
            android:name=".Withdrawal"
            android:theme="@style/Theme.MyDialog" />
    </application>

</manifest>